<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <servlet>
        <servlet-name>HelloCookie</servlet-name>
        <servlet-class>cookie_learning.HelloCookie</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>HistCookie</servlet-name>
        <servlet-class>cookie_learning.HistTime</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>SessionDemo1</servlet-name>
        <servlet-class>session_learning.SessionDemo1</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>DetailServlet</servlet-name>
        <servlet-class>cookie_learning.HistProduct.servlet.DetailServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>ListServlet</servlet-name>
        <servlet-class>cookie_learning.HistProduct.servlet.ListServlet</servlet-class>
    </servlet>



    <servlet-mapping>
        <servlet-name>HelloCookie</servlet-name>
        <url-pattern>/HelloCookie</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>HistCookie</servlet-name>
        <url-pattern>/HistCookie</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>SessionDemo1</servlet-name>
        <url-pattern>/SessionDemo1</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>ListServlet</servlet-name>
        <url-pattern>/ListServlet</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>DetailServlet</servlet-name>
        <url-pattern>/DetailServlet</url-pattern>
    </servlet-mapping>
</web-app>